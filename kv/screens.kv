      ActionView:
        ActionPrevious:
          ActionButton:
            on_press: app.root.current = 'news_screen' 
            with_previous: True
        ActionButton:
          icon: 'atlas://data/images/defaulttheme/audio-volume-high'
          on_press: 
        ActionButton:
          important: True
          text: 'middle'
          on_press: app.root.current = 'middle_screen'
        ActionButton:
          text: 'Records'
          on_press: app.root.current = 'records_screen'
        ActionButton:
          text: 'News'
          on_press: app.root.current = 'news_screen' 
        ActionButton:
          text: 'Others'
