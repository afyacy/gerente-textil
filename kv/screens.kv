

<MiddleScreen>:
  name: "middle_screen"
  BoxLayout:
    orientation: 'vertical'
    background_color: COLOR("#0066BA")
    ActionBar:
      use_separator: True
      ActionView:
        ActionPrevious:
          on_press: app.root.current = 'middle_screen'
          SearchBar:
            size_hint_x: 1.7
            size_hint_y: .7
            pos_hint: {'x': 3, 'center_y':.5}
            on_text_validate: searchbutt.trigger_action()
        ActionButton:
          icon: 'images/searchIcon.png'
          on_press: 
        ActionButton:
          important: True
          text: 'Home'
          on_press: app.root.current = 'middle_screen'
        ActionButton:
          text: 'Records'
          on_press: app.root.current = 'records_screen'
        ActionButton:
          text: 'News'
          on_press: app.root.current = 'news_screen' 
        ActionButton:
          text: 'Others'
          on_press:  app.root.current = 'others_screen' 
        ActionOverflow
          ActionButton:
            text: 'GTP'
            on_press:  app.root.current = 'gtp_screen' 
          ActionButton:
            text: 'Woodin'
            on_press:  app.root.current = 'woodin_screen' 
          ActionButton:
            text: 'Holland'
            on_press: app.root.current = 'holland_screen' 
